	private void writeOutputFile(String s){
		try {
			if(out == null){
				out = new FileOutputStream(new File("output.txt"));
				dos = new DataOutputStream(out);
				
				s = s + "\r\n";
				byte[] data = s.getBytes("UTF-8");
				dos.write(data);
			}
			else{
				s = s + "\r\n";
				byte[] data = s.getBytes("UTF-8");
				dos.write(data);
			}
		} 
		catch (Exception e){
			e.printStackTrace();
		}
	}


